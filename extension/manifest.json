{
    "manifest_version": 3,
    "name": "Any2Api",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
        "cookies",
        "storage"
      ],
      "host_permissions": [
        "http://*/*","https://*/*"
      ]
  }